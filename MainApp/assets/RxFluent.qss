QWidget#MainDock {
    background-color: rgba(0, 0, 0, 60%);
    background-image: url(:/imgs/assets/workspace_is_empty.png);
    background-position: center;
    background-repeat: no-repeat;
    border: 1px solid black;
    border-left: 0px solid black;
}

QSplitter::handle {
    background-color: rgb(30, 30, 30);
}

QSplitter::handle:horizontal {
    width: 3px;
}

QSplitter::handle:vertical {
    height: 3px;
}

QSplitter::handle:pressed {
    background-color: rgb(50, 250, 150);
}

QSplitter::handle:hover {
    background-color: rgb(50, 150, 250);
}

QStatusBar {
    background-color: rgba(30, 30, 30, 60%);
}

QTextEdit {
    color: white;
    background-color: #2c2c2c;
    border: 0px solid black;
}

QScrollBar {
    background-color: #2c2c2c;
    border: 0px solid black;
}

QScrollBar::handle {
    background-color: #3e3e3e;
}

QScrollBar::handle:hover {
    background-color: #444444;
}

QScrollBar::add-line {
    height: 0px;
}

QScrollBar::sub-line {
    height: 0px;
}

QLabel#isaeLabel {
    color: rgb(48, 172, 255);
    font: 18pt "Earth Orbiter";
}

QLabel#isaeLabel:hover {
    color: rgb(42, 216, 255);
}

QLabel#aboutChineseLabel {
    color: rgb(48, 172, 255);
