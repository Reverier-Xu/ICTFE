QMainWindow {
    border: 1px solid black;
    background-image: url(:/imgs/wallpaper);
}

QMenu {
    background-color: rgba(30, 30, 30, 70%);
    border: 1px solid #000000;
}

QMenu::item {
    background-color: transparent;
}

QMenu::item:selected {
    color: rgb(26, 125, 255);
    background-color: rgba(90, 90, 90, 30%);
}

QWidget#centralwidget {
    background-color: transparent;
}

QWidget#sidebarWidget {
    background-color: rgba(30, 30, 30, 60%);
    border: 1px solid black;
}

QWidget#appsArea {
    background-color: rgba(140, 140, 140, 15%);
    border: 1px solid black;
}

QWidget#teamArea {
    background-color: rgba(140, 140, 140, 15%);
    border: 1px solid black;
}

QWidget#workspaceArea {
    background-color: rgba(140, 140, 140, 15%);
    border: 1px solid black;
}

QWidget#titleWidget {
    background-color: rgba(30, 31, 32, 60%);
    border: 0px solid black;
    border-top: 1px solid black;
    border-left: 1px solid black;
    border-right: 1px solid black;
}

QToolButton {
    background-color: transparent;
    color: white;
    border: 0px solid black;
}

QToolButton:hover {
    color: #3e79fc;
}

QToolButton:pressed {
    color: #3e79ff;
}

QLineEdit {
    background-color: transparent;
    border: 1px solid rgba(100, 100, 100, 50%);
    /*border-top: 1px solid black;*/
    margin: 0 0 0 0;
}

QLineEdit#globalSearchBar {
    background-color: transparent;
    border: 0px solid black;
    border-top: 1px solid black;
    margin: 0 0 0 0;
}

QLineEdit#globalSearchBar:hover {
    background-color: rgba(100, 100, 100, 40%);
}

QLineEdit#globalSearchBar:focus {
    background-color: rgba(130, 130, 130, 20%);
}

QLineEdit:hover {
    background-color: rgba(100, 100, 100, 40%);
}

QLineEdit:focus {
    background-color: rgba(130, 130, 130, 20%);
}

